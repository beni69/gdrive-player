{
    "name": "gdrive-player",
    "version": "1.0.0",
    "description": "a remote music player that uses google drive as a database and admin panel",
    "main": "src/index.ts",
    "engines": {
        "node": "^16"
    },
    "scripts": {
        "check": "tsc --noemit",
        "build": "node build.js",
        "export": "npm run build && pkg -t node16 -c pkg.json --compress brotli -o dist/player dist/bundle.cjs",
        "start": "tsm ."
    },
    "keywords": [],
    "author": "",
    "license": "GPL-3.0",
    "dependencies": {
        "@googleapis/drive": "^2.3.0",
        "@suldashi/lame": "^1.2.5",
        "speaker": "^0.5.4"
    },
    "devDependencies": {
        "@types/node": "^17.0.31",
        "esbuild": "^0.14.38",
        "pkg": "^5.6.0",
        "tsm": "^2.2.1",
        "typescript": "^4.6.4"
    }
}
